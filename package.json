{
  "name": "temp-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "author": "Medical AI Team",
  "keywords": [
    "medical",
    "ai",
    "offline",
    "privacy",
    "healthcare"
  ],
  "scripts": {
    "postinstall": "bash install-ollama.sh --auto --quiet || echo 'Ollama setup skipped in CI environment'",
    "setup": "bash install-ollama.sh",
    "setup:full": "bash install-ollama.sh --full",
    "dev": "npm run ensure-ollama && vite",
    "dev-with-proxy": "node proxy-server.js & vite",
    "build": "tsc && vite build",
    "build:tauri": "npm run ensure-ollama && tauri build",
    "preview": "vite preview",
    "tauri": "tauri",
    "ensure-ollama": "bash scripts/ensure-ollama.sh",
    "start-ollama": "ollama serve > /tmp/ollama.log 2>&1 &",
    "test": "bash scripts/health-check.sh",
    "test:frontend": "bash scripts/test-frontend.sh",
    "test:backend": "bash scripts/test-backend.sh",
    "test:build": "bash scripts/test-build.sh",
    "test:dev": "bash scripts/test-dev.sh",
    "test:api": "bash scripts/test-api.sh",
    "test:all": "bash scripts/test-all.sh",
    "guide": "bash scripts/developer-guide.sh"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "@tauri-apps/api": "^2.5.0",
    "@tauri-apps/plugin-opener": "^2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.5",
    "tailwindcss": "^4.1.10",
    "typescript": "~5.6.2",
    "vite": "^6.0.3"
  }
}
